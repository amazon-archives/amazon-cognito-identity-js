  "author": {
    "name": "Amazon Web Services",
    "email": "aws@amazon.com",
    "url": "http://aws.amazon.com"
  },
  "homepage": "http://aws.amazon.com/cognito",
  "contributors": [
    "Simon Buchan with Skilitics",
    "Jonathan Goldwasser",
    "Matt Durant",
    "John Ferlito",
    "Michael Hart",
    "Tylor Steinberger",
    "Paul Watts",
    "Gleb Promokhov",
    "Chetan Mehta <chetam@amazon.com>",
